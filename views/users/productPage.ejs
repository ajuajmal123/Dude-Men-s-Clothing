  <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>

  <div id="wishlistMessage" style="display:none; position: fixed; top: 10px; right: 10px; background-color: #4CAF50; color: white; padding: 10px; border-radius: 5px;">
    Product added to wishlist!
  </div>
  
  <script>
    // Function to display the message
    function showWishlistMessage(message) {
      const messageDiv = document.getElementById('wishlistMessage');
      messageDiv.innerHTML = message;
      messageDiv.style.display = 'block';
      
      // Hide the message after 2 seconds
      setTimeout(() => {
        messageDiv.style.display = 'none';
      }, 2000);
    }
  
    // Function to add product to wishlist
    async function addToWishlist(userId, productId, element) {
      event.preventDefault();
      try {
        const response = await axios.post(`/addtowishlist/${productId}`, { userId });
        if (response.data.success) {
          // Show success message
          showWishlistMessage('Product added to wishlist!');
          
          // Change the icon to filled heart
          element.innerHTML = '<i class="fi-rs-heart-fill"></i>';
          element.setAttribute('onclick', `removeFromWishlist('${userId}', '${productId}', this)`);
        } else {
          alert(response.data.message);  // Display error message
        }
      } catch (error) {
        console.error('Error adding to wishlist:', error);
        alert('Failed to add product to wishlist');
      }
    }
  
    // Function to remove product from wishlist
    async function removeFromWishlist(userId, productId, element) {
      event.preventDefault();
      try {
        const response = await axios.post(`/removefromwishlist`, { userId, productId });
        if (response.data.success) {
          // Show success message
          showWishlistMessage('Product removed from wishlist!');
          
          // Change the icon back to empty heart
          element.innerHTML = '<i class="fi-rs-heart"></i>';
          element.setAttribute('onclick', `addToWishlist('${userId}', '${productId}', this)`);
        } else {
          alert('Failed to remove product from wishlist.');
        }
      } catch (error) {
        console.error('Error removing from wishlist:', error);
        alert('Failed to remove product from wishlist.');
      }
    }
  </script>
   
