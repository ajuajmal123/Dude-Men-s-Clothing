        </header>
        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">Users list</h2>
                
            </div>
            <div class="card mb-4">
                <header class="card-header">
                  
                </header> <!-- card-header end// -->
                <form class="search-container d-flex p-0 w-50 ms-auto" action="/admin/userlist" method="get">
                    <input type="text" class="flex-grow-1" placeholder="search..." name="search" value="<%= search %>"style="font-size: 15px; text-align: right;" />
                    
                    <button class="btn btn-primary h-100 rounded-start-0">Search</button>
                </form>
                
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th scope="col">Sr Num</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    
                                    <th scope="col">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% let serialNumber=1; %>
                                    <% users.forEach(user=>{ %>
                                        <tr>
                                            <td scope="row">
                                                <%= serialNumber %>
                                            </td>
                                            <td>
                                                <%= user.name%>
                                            </td>
                                            <td>
                                                <%= user.email%>
                                            </td>
                                           
                                            <td>
                                                <%= user.blocked? 'Blocked' :'Active' %>
                                            </td>
                                            <td>
                                                <form id="blockForm_<%= user.id %>" method="post"
                                                    action="/admin/block/<%= user._id %>"
                                                    onsubmit="return confirmBlock(this);">
                                                    <button type="submit" class="btn btn-primary"
                                                        data-username="<%= user.name %>"
                                                        data-action="<%= user.blocked? 'Unblock': 'Block' %>">
                                                        <%= user.blocked? 'Unblock' : 'Block' %>


                                                    </button>
                                                </form>
                                            </td>
                                        </tr>
                                        <% serialNumber++; %>
                                            <% }); %>
                            </tbody>
                        </table> <!-- table-responsive.// -->
                    </div>
                </div> <!-- card-body end// -->
            </div> 

        </section> <!-- content-main end// -->
